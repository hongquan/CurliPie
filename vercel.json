{
  "version": 2,
  "public": true,
  "functions": {
    "api/main.py": {
      "excludeFiles": "_public"
    }
  },
  "rewrites": [
    {
      "source": "/:path(redoc|docs|openapi.json)*",
      "destination": "/api/main.py"
    },
    {
      "source": "/static/:match*",
      "destination": "/api/main.py"
    }
  ]
}
